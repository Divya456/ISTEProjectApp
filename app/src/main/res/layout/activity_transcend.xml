<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.bk.iste.Transcend">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="EVENTS"
        android:id="@+id/textView7"
        android:textSize="36dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Civil Event"
        android:id="@+id/civil"
        android:textSize="24dp"
        android:layout_below="@+id/botstaclerace"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Botstacle Race"
        android:id="@+id/botstaclerace"
        android:textSize="24dp"
        android:layout_below="@+id/crysis"
        android:layout_alignLeft="@+id/crysis"
        android:layout_alignStart="@+id/crysis" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Git Workshop"
        android:id="@+id/git"
        android:textSize="24dp"
        android:layout_below="@+id/civil"
        android:layout_alignLeft="@+id/botstaclerace"
        android:layout_alignStart="@+id/botstaclerace" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Crysis"
        android:id="@+id/crysis"
        android:textSize="24dp"
        android:layout_below="@+id/textView7"
        android:layout_marginTop="46dp"
        android:layout_alignRight="@+id/spectacle"
        android:layout_alignEnd="@+id/spectacle"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Serial Coder"
        android:id="@+id/serialcoder"
        android:layout_below="@+id/git"
        android:textSize="24dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Spectacle"
        android:id="@+id/spectacle"
        android:textSize="24dp"
        android:layout_below="@+id/serialcoder"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
</RelativeLayout>
